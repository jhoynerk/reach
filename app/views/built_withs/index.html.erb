<p id="notice"><%= notice %></p>

<h1>Listing Built Withs</h1>

<table>
  <thead>
    <tr>
      <th>Domain</th>
     <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @built_withs.each do |built_with| %>
      <tr>
        <td><%= built_with.domain %></td>
       <td><%= link_to 'Show', built_with %></td>
        <td><%= link_to 'Edit', edit_built_with_path(built_with) %></td>
        <td><%= link_to 'Destroy', built_with, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Built with', new_built_with_path %>
